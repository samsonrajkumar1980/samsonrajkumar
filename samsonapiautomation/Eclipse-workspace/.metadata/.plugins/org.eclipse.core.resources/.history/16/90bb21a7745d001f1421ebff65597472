package urls;

public enum urls {
	
	baseUrl("https://reqres.in/"),
	createpostapi("api/users"),
	retrievegetapi("api/users?page={userid}"),
	validateunknownapi("api/unknown");
	
	private String endpoint;
	urls(String endpoint)
	{
		this.endpoint=endpoint;
	}
	
	public String endpoint()
	{
		return endpoint;
	}

}
